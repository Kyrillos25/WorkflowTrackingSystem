{
  "ConnectionStrings": {
    "Database": "",
    "Cache": "",
    "Queue": ""
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "WorkflowTracking.WorkflowManagement.Api"
    }
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "",
  "Users": {
    "Outbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 50
    },
    "Inbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 50
    }
  }
}